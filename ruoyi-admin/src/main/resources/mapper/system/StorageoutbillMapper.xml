            <if test="cTime != null ">c_time,</if>
            <if test="uTime != null ">u_time,</if>
            <if test="delStatus != null ">del_status,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="storageoutid != null  and storageoutid != ''">#{storageoutid},</if>
