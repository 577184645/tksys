            <if test="uTime != null ">#{uTime},</if>
            <if test="delStatus != null ">#{delStatus},</if>
         </trim>
    </insert>

    <update id="updateStorageoutbill" parameterType="Storageoutbill">
        update storageoutbill
        <trim prefix="SET" suffixOverrides=",">
